<!DOCTYPE html>
<html lang="en">
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta charset="utf-8">
  <title>{{ options.title }}</title>
  <meta name="description" content="">
  <meta name="generator" content="kss-node">
  <link rel="shortcut icon" href="" type="image/vnd.microsoft.icon" />
  <link href="https://fonts.googleapis.com/css?family=Poppins:400,600,700&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
  {{ styles|raw }}
  <link rel="stylesheet" href="kss-assets/lib/docs.css">
  <link rel="stylesheet" href="assets/css/styleguide.css">
</head>
<body id="" {% if template.isItem %}class="fullscreen-mode"{% endif %}>
  <div class="layout-container">
    <a href="#main-content" class="sr-only sr-only-focusable"> Skip to main content </a>
  {% if not template.isItem %}
    <div class="page-navigation" id="navigation" style="top:0px">
      <nav role="navigation" aria-labelledby="block-mainnavigation-menu" class="navbar navbar-expand-lg navbar-light">
        <div class="container">
          <a class="navbar-brand" href="./" title="Home" rel="home">
            <img src="assets/images/logo.svg" alt="{{ options.title }}" />
          </a>
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav">
              {% for menu_item in menu %}
                <li class="nav-item"><a class="nav-link{% if menu_item.isActive %} is-active {% endif %}" href="section-{{ menu_item.referenceURI }}.html">{{ menu_item.header }}</a></li>
              {% endfor %}
            </ul>
          </div>
        </div>
      </nav>
    </div>
  {% endif %}
    <div class="container{% if template.isItem %}-fluid{% endif %}">
      <div class="row flex-xl-nowrap">
        {% if not template.isItem and not homepage %}
          <div class="d-none d-md-block col-md-3 col-xl-2 bd-toc" id="scrollspy" >
            {% for menu_item in menu %}
              {% if menu_item.isActive and menu_item.children is not empty %}
                <div class="btn-group btn-group-sm float-right" role="group" aria-label="Example toolbar">
                  <a href="#" onclick="event.preventDefault();" data-kss-guides="true" class="btn btn-light btn-guides">
                    <span class="kss-toolbar__tooltip sr-only">Toggle example guides</span>
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                      <g id="toggle-guides" fill="none" fill-rule="evenodd">
                        <path id="Shape" fill="#000" fill-rule="nonzero" d="M6,3 L8,3 L8,4 L6,4 L6,3 Z M9,3 L11,3 L11,4 L9,4 L9,3 Z M14,3 L14,7 L11,7 L11,6 L13,6 L13,4 L12,4 L12,3 L14,3 Z M5,6 L7,6 L7,7 L5,7 L5,6 Z M8,6 L10,6 L10,7 L8,7 L8,6 Z M3,4 L3,6 L4,6 L4,7 L2,7 L2,3 L5,3 L5,4 L3,4 Z M6,9 L8,9 L8,10 L6,10 L6,9 Z M9,9 L11,9 L11,10 L9,10 L9,9 Z M14,9 L14,13 L11,13 L11,12 L13,12 L13,10 L12,10 L12,9 L14,9 Z M5,12 L7,12 L7,13 L5,13 L5,12 Z M8,12 L10,12 L10,13 L8,13 L8,12 Z M3,10 L3,12 L4,12 L4,13 L2,13 L2,9 L5,9 L5,10 L3,10 Z M15,1 L1,1 L1,15 L15,15 L15,1 Z M16,0 L16,16 L0,16 L0,0 L16,0 Z"/>
                      </g>
                    </svg>
                  </a>
                  <a href="#" onclick="event.preventDefault();" data-kss-markup="true" class="btn btn-light btn-markup">
                    <span class="kss-toolbar__tooltip sr-only">Toggle HTML markup</span>
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                      <g id="toggle-markup" fill="none" fill-rule="evenodd">
                        <polygon id="Shape" fill="#000" fill-rule="nonzero" points="10.526 11.711 11.789 12.974 16 8.763 11.789 4.553 10.526 5.816 13.474 8.763"/>
                        <polygon id="Shape" fill="#000" fill-rule="nonzero" points="5.474 5.816 4.211 4.553 0 8.763 4.211 12.974 5.474 11.711 2.526 8.763"/>
                        <polygon id="Shape" fill="#000" fill-rule="nonzero" points="8.807 4.007 9.721 4.256 7.194 13.52 6.28 13.27 8.807 4.007"/>
                      </g>
                    </svg>
                  </a>
                </div>

                <ul class="section-nav nav">
                  <li class="toc-entry toc-h1 nav-item"><a href="#main-content" class="h6 text-secondary"><span class="nav__name">{{ menu_item.header }}</span></a></li>
                  {% for menu_child in menu_item.children %}
                    <li class="toc-entry toc-h2 nav-item">
                      <a class="menu-link nav-link" href="#{{ menu_child.referenceURI }}">
                        <span class="ref sr-only{% if menu_child.isGrandChild %} child{% endif %}">{{ menu_child.referenceNumber }}</span> <span class="nav__name">{{ menu_child.header }}</span>
                      </a>
                    </li>
                  {% endfor %}
                  <li class="toc-entry toc-h2">
                    <a href="#main-content" class="back-to-top">
                      <span>Back to top</span>
                    </a>
                  </li>
                </ul>
              {% endif %}
            {% endfor %}
          </div>
        {% endif %}
      {% if homepage %}
        <main id="main" class="bd-content" role="main">
          {{ homepage|raw }}
        </main>
      {% else %}
        <main class="col-12 {% if not template.isItem %}col-md-9 col-xl-10 py-md-3 {% endif %}bd-content" role="main">
          <a id="main-content" tabindex="-1"></a>
            {#
              Display each section, in order.

              The "root" element comes first in this loop, and can be detected using the
              "loop.first" variable as seen below.
              "loop.first" variable as seen below.
            #}
            {% for section in sections %}
            {% set sectionElement = loop.first ? 'div' : 'section' %}
            <{{ sectionElement }} id="{{ section.referenceURI }}" class="section section--depth-{{ section.depth }} {% if template.isItem %}is-fullscreen{% endif %}">
              {% set headerElement = (section.depth > 6) ? 'h6' : 'h' ~ section.depth %}
              <{{ headerElement }} class="{% if section.depth == 1 %}bd-title{% endif %}">
                {{ section.header }}
                <a href="#{{ section.referenceURI }}" class="text-muted">
                  <small>#
                    <span class="sr-only">
                      {{ section.referenceNumber }} {{ hasNumericReferences ? '#permalink' : section.reference }}
                    </span>
                  </small>
                </a>
              </{{ headerElement }}>

            {% if section.example %}
              <div class="btn-group btn-group-sm bd-toolbar" role="group" aria-label="Example toolbar">
                {% if not template.isItem %}
                  <a href="#{{section.referenceURI}}" onclick="event.preventDefault();" data-kss-fullscreen="{{section.referenceURI}}" class="btn btn-light btn-fullscreen">
                    <span class="kss-toolbar__tooltip sr-only">Toggle full screen</span>
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                      <g id="toggle-full-screen" fill="none" fill-rule="evenodd">
                        <path id="Shape" fill="#000" fill-rule="nonzero" d="M16,0 L16,6.5 L13.5,4 L10.5,7 L9,5.5 L12,2.5 L9.5,0 L16,0 Z M7,10.5 L4,13.5 L6.5,16 L0,16 L0,9.5 L2.5,12 L5.5,9 L7,10.5 Z"/>
                      </g>
                    </svg>
                  </a>
                  <a href="item-{{section.referenceURI}}.html" target="_blank" class="btn btn-light btn-newwindow">
                    <span class="kss-toolbar__tooltip sr-only">Open in new window</span>
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                      <g id="new-tab" fill="none" fill-rule="evenodd">
                        <path id="Shape" fill="#000" fill-rule="nonzero" d="M2.85714286,2 L2.85714286,13.1428571 L14,13.1428571 L14,2 L2.85714286,2 Z M13.0714286,12.2142857 L3.78571429,12.2142857 L3.78571429,2.92857143 L13.0714286,2.92857143 L13.0714286,12.2142857 Z M1.92857143,14.0714286 L1.92857143,4.32142857 L1,3.39285714 L1,15 L12.6071429,15 L11.6785714,14.0714286 L1.92857143,14.0714286 Z"/>
                        <polygon id="Shape" fill="#000" fill-rule="nonzero" points="5.179 4.786 7.5 7.107 4.714 9.893 6.107 11.286 8.893 8.5 11.214 10.821 11.214 4.786"/>
                      </g>
                    </svg>
                  </a>
                {% endif %}
                <a href="#{{section.referenceURI}}" onclick="event.preventDefault();" data-kss-guides="true" class="btn btn-light btn-guides">
                  <span class="kss-toolbar__tooltip sr-only">Toggle example guides</span>
                  <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                    <g id="toggle-guides" fill="none" fill-rule="evenodd">
                      <path id="Shape" fill="#000" fill-rule="nonzero" d="M6,3 L8,3 L8,4 L6,4 L6,3 Z M9,3 L11,3 L11,4 L9,4 L9,3 Z M14,3 L14,7 L11,7 L11,6 L13,6 L13,4 L12,4 L12,3 L14,3 Z M5,6 L7,6 L7,7 L5,7 L5,6 Z M8,6 L10,6 L10,7 L8,7 L8,6 Z M3,4 L3,6 L4,6 L4,7 L2,7 L2,3 L5,3 L5,4 L3,4 Z M6,9 L8,9 L8,10 L6,10 L6,9 Z M9,9 L11,9 L11,10 L9,10 L9,9 Z M14,9 L14,13 L11,13 L11,12 L13,12 L13,10 L12,10 L12,9 L14,9 Z M5,12 L7,12 L7,13 L5,13 L5,12 Z M8,12 L10,12 L10,13 L8,13 L8,12 Z M3,10 L3,12 L4,12 L4,13 L2,13 L2,9 L5,9 L5,10 L3,10 Z M15,1 L1,1 L1,15 L15,15 L15,1 Z M16,0 L16,16 L0,16 L0,0 L16,0 Z"/>
                    </g>
                  </svg>
                </a>
                <a href="#{{section.referenceURI}}" onclick="event.preventDefault();" data-kss-markup="true" class="btn btn-light btn-markup">
                  <span class="kss-toolbar__tooltip sr-only">Toggle HTML markup</span>
                  <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                    <g id="toggle-markup" fill="none" fill-rule="evenodd">
                      <polygon id="Shape" fill="#000" fill-rule="nonzero" points="10.526 11.711 11.789 12.974 16 8.763 11.789 4.553 10.526 5.816 13.474 8.763"/>
                      <polygon id="Shape" fill="#000" fill-rule="nonzero" points="5.474 5.816 4.211 4.553 0 8.763 4.211 12.974 5.474 11.711 2.526 8.763"/>
                      <polygon id="Shape" fill="#000" fill-rule="nonzero" points="8.807 4.007 9.721 4.256 7.194 13.52 6.28 13.27 8.807 4.007"/>
                    </g>
                  </svg>
                </a>
              </div>
            {% endif %}


            {% if section.description %}
              <div class="description{% if section.depth == 1 %} bd-lead{% endif %}">
                {{ section.description|raw }}
              </div>
            {% endif %}

            {% for parameter in section.parameters %}
              {% if loop.first %}
                <div class="parameters__title">Parameters:</div>
                <ul class="parameters">
              {% endif %}
              <li class="parameters__item">
                <div class="parameters__name"><code>{{ parameter.name }}</code></div>
                <div class="parameters__description">
                  {{ parameter.description|raw }}
                  {% if parameter.defaultValue %}
                    <div class="parameters__default-value">
                      Defaults to: <code>{{ parameter.defaultValue }}</code>
                    </div>
                  {% endif %}
                </div>
              </li>
              {% if loop.last %}
                </ul>
              {% endif %}
            {% endfor %}


            {% if section.example %}
              <div class="bd-example">

                  {% if section.modifiers is not empty %}
                    <!--<div class="kss-modifier__default-name kss-style">
                      Default styling
                    </div>-->
                  {% endif %}

                  {% if section.modifiers is empty %}
                    <div class="modifier__example">
                      {{ section.example|raw }}
                      <div class="modifier__example-footer"></div>
                    </div>
                  {% endif %}

                  {% for modifier in section.modifiers %}
                    <code class="modifier__name kss-style">
                      {{ modifier.name }}
                    </code>
                    <div class="modifier__description kss-style">
                      {{ modifier.description|raw }}
                    </div>
                    <div class="modifier__example">
                      {{ modifier.markup|raw }}
                    </div>
                  {% endfor %}

              </div>

              {% if section.markup %}
                <details class="highlight">
                  <summary style="padding: 5px 20px; cursor: pointer;">
                    {% if section.markupFile %}
                      Markup: <code>{{ section.markupFile }}</code>
                    {% else %}
                      Markup
                    {% endif %}
                  </summary>
                  <pre><code class="language-markup" data-language="html">{{ section.markup|escape('html') }}</code></pre>

                </details>
              {% endif %}

            {% endif %}

            {% if section.source.filename and section.markup %}
              <small class="text-muted">
                Source: <code>{{ section.source.filename }}</code>, line {{ section.source.line }}
              </small>
            {% endif %}
          </{{ sectionElement }}>
          {% endfor %}

        </main>
      {% endif %}
      </div>
    </div>
  </div>

  <!-- SCRIPTS -->


  <script src="kss-assets/lib/kss-fullscreen.js"></script>
  <script src="kss-assets/lib/kss-guides.js"></script>
  <script src="kss-assets/lib/kss-markup.js"></script>
  <script src="kss-assets/lib/prism.js"></script>
  <script src="kss-assets/lib/prism-scss.min.js"></script>

  {{ scripts|raw }}
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
  
  <script>
    // Escape code
    // ============================================================

    var $lang_markup       = document.querySelectorAll('pre.language-html code'),
        $lang_js           = document.querySelectorAll('script.atomic-js'),
        $lang_jscode       = document.querySelectorAll('pre code.language-javascript'),
        lang_markupsnippet = [],
        lang_jsnippet      = [];

    // Markup
    // order matters!
    // ============================================================

    for(var i = 0, l = $lang_markup.length; i < l; i++) {
      lang_markupsnippet.push(
        $lang_markup[i].innerHTML.replace(/&/g, '&amp;').replace(/</g, '&lt;').replace(/>/g, '&gt;')
      );
      $lang_markup[i].innerHTML = lang_markupsnippet[i];
    }

    // JavaScript
    // order matters!
    // ============================================================
    for(var i = 0, l = $lang_jscode.length; i < l; i++) {
      lang_jsnippet.push(
        $lang_jscode[i].innerHTML.replace(/&/g, '&amp;').replace(/</g, '&lt;').replace(/>/g, '&gt;')
      );
    }

    for(var i = 0, l = $lang_js.length; i < l; i++) {
      $lang_js[i].innerHTML = lang_jsnippet[i];
    }

    var KssFullScreen = new KssFullScreen({
      idPrefix: 'fullscreen-',
      bodyClass: 'kss-fullscreen-mode',
      elementClass: 'is-fullscreen'
    });
    var kssGuides = new KssGuides({
      bodyClass: 'guides-mode'
    });
    var kssMarkup = new KssMarkup({
      bodyClass: 'markup-mode',
      detailsClass: 'highlight'
    });

  </script>

  <script src="kss-assets/styleguide.js"></script>
</body>
</html>
